<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="remote.wise.businessentity.EmailPendingEntity" table="email_pending">
		<id name="reqId" type="int">
			<column default="0" name="req_id" />
			<generator class="identity" />
		</id>
		<property generated="never" lazy="false" name="emailId"
			type="java.lang.String">			
			<column name="email_id" />
		</property>
		<property generated="never" lazy="false" name="emailSubject"
			type="java.lang.String">
			<column name="email_subject" />
		</property>
		<property generated="never" lazy="false" name="emailBody"
			type="java.lang.String">
			<column name="email_body" />
		</property>
		<property generated="never" lazy="false" name="generatedTime"
			type="java.sql.Timestamp">
			<column name="generated_time" />
		</property>
	</class>
</hibernate-mapping>
